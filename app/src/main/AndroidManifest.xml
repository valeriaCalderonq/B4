<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.valeria.bancolino">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CuentaBancariaActivity"
            android:label="@string/title_activity_cuenta_bancaria"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CuentaActivity" />
        <activity android:name=".TransaccionesActivity" />
        <activity android:name=".PagoActivity" />
        <activity android:name=".AguaActivity" />
        <activity android:name=".LuzActivity" />
        <activity android:name=".TelefonoActivity" />
        <activity android:name=".UniversidadActivity" />
        <activity android:name=".SueldoActivity" />
        <activity android:name=".ConfirmacionSueldo" />
    </application>

</manifest>